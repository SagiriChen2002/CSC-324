# Parse the 'Time' column to extract and convert start and end times
data <- data %>%
mutate(
# Split the 'Time' column into components: date, start_time, end_time
Time_split = strsplit(as.character(TIME), ", | - |PM|AM", perl=TRUE),
Date = sapply(Time_split, function(x) paste(x[1], x[2], "2024")), # Append the year for proper parsing
start_time_str = sapply(Time_split, function(x) paste(x[3], ifelse(grepl("AM", x[4]), "AM", "PM"))),
end_time_str = sapply(Time_split, function(x) paste(x[5], ifelse(grepl("PM", x[6]), "PM", "AM")))
) %>%
mutate(
# Combine the date with start and end times and parse
start_time = mdy_hm(paste(Date, start_time_str)),
end_time = mdy_hm(paste(Date, end_time_str))
)
# Optionally calculate duration
data$duration <- as.numeric(difftime(data$end_time, data$start_time, units="mins"))
# Convert start_time to a date (without time) for grouping
data$Date <- as.Date(data$start_time)
# Aggregate data: calculate average start time (as hour of the day) and total duration per day
daily_summary <- data %>%
group_by(Week, Date) %>%
summarise(
avg_start_hour = mean(hour(start_time) + minute(start_time) / 60),
total_duration = sum(duration)
) %>%
ungroup()
# Plot the summary data
ggplot(daily_summary, aes(x=Date, y=avg_start_hour, group=Week)) +
geom_line(aes(color=factor(Week))) +
geom_point(aes(size=total_duration)) +
scale_size(range = c(1, 6), name="Total Duration (mins)") +
labs(title="Weekly Time Log Summary", x="Date", y="Average Start Time (hour)", color="Week") +
theme_minimal()
